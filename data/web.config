<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpErrors>
            <remove statusCode="404" subStatusCode="-1" />
            <error statusCode="404" prefixLanguageFilePath="" path="/error/404.htm" responseMode="File" />
        </httpErrors>
        <handlers accessPolicy="Read, Execute, Script" />
    </system.webServer>
</configuration>
